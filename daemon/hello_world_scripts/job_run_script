#!/bin/sh
/bin/cat /proc/self/status | /bin/grep -e "^PPid" | /usr/bin/cut -f2 > running

/bin/sleep 10
echo -n "Hello_World from " > LGI_output
/bin/sleep 20
/bin/cat LGI_target_resources >> LGI_output
/bin/sleep 40
echo -n " submitted with input: " >> LGI_output
/bin/sleep 80
/bin/cat LGI_input >> LGI_output
/bin/sleep 160

# /home/mark/temp/LGI/daemon/bin/LGI_qsub -a hello_world -i LGI_target_resources >> LGI_output

/bin/touch finished
/bin/rm running
